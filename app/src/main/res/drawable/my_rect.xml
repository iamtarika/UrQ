<?xml version="1.0" encoding="utf-8"?>



<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke android:color="#fafafa" android:width="3dp"/>
            <corners android:radius="1dp" />
        </shape>
    </item>

    <item
        android:left="0dp"
        android:right="0dp"
        android:top="0dp"
        android:bottom="0dp">
        <shape android:shape="rectangle">
            <stroke android:color="#f6f6f6" android:width="2dp"/>
            <corners android:radius="3dp" />
        </shape>
    </item>
</layer-list>